{
  "name": "aanro/aanro-wechat",
  "description": "Someline Wechat is a part of Someline Monster Components.",
  "keywords": [
    "wechat",
    "service",
    "someline-wechat",
    "starter-framework",
    "someline-starter",
    "laravel"
  ],
  "homepage": "https://gitee.com/aanro/someline-wechat.git",
  "license": "MIT",
  "authors": [
    {
      "name": "Libern Lin",
      "email": "libernlin@gmail.com",
      "homepage": "https://www.someline.com/"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*",
    "overtrue/laravel-wechat": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "Someline\\Component\\Wechat\\": "src/Someline/Component/Wechat"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Someline\\Components\\Wechat\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "dont-discover": [
        "overtrue/laravel-wechat"
      ],
      "providers": [
        "Someline\\Component\\Wechat\\LaravelWechatServiceProvider",
        "Someline\\Component\\Wechat\\SomelineWechatServiceProvider"
      ]
    }
  }
}
